//  This code does not execute properly. Try to figure out why.

public class Multiply {
    public static Double multiply(Double a, Double b) {
        return (a == null || b == null) ? null : a * b;
    }
}

//SEGUNDA SOLUÇÃO:
public class Multiply {
    public static Double multiply(Double a, Double b) {
        return a * b;
    }
}
